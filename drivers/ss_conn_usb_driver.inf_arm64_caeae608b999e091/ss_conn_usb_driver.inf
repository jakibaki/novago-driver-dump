[ssud.Install.NTarm64.CoInstallers]
AddReg=CoInstaller_AddReg_NTarm64
CopyFiles=CoInstaller_CopyFiles_NTarm64

[CoInstaller_CopyFiles_NTarm64]
; No CopyFiles

[CoInstaller_AddReg_NTarm64]
; No AddReg

[ss_conn_usb_driver_wdfsect]
KmdfLibraryVersion = 1.7

[ssud.AddService]
DisplayName    = %ssud.SvcDesc%
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 3                  ; SERVICE_DEMAND_START
ErrorControl   = 1                  ; SERVICE_ERROR_NORMAL
ServiceBinary  = %10%\System32\Drivers\ss_conn_usb_driver.sys
LoadOrderGroup = Base

[SourceDisksNames.x86]
1=%Disk_Description%,,,\i386

[SourceDisksNames.amd64]
