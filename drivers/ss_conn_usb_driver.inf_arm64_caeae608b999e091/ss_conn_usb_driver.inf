[ssud.Install.NT]
CopyFiles=ssud.NT.CopyFiles

[ssud.Install.NT.Services]
AddService = ss_conn_usb_driver, %SPSVCINST_ASSOCSERVICE%, ssud.AddService

[ssud.NT.CopyFiles]
ss_conn_usb_driver.sys

[ssud.Install.NT.Wdf]
KmdfService = ss_conn_usb_driver, ss_conn_usb_driver_wdfsect

[ssud.Install.CoInst.NT]
CopyFiles=ssud.NT.CopyFiles

[ssud.Install.CoInst.NT.Services]
AddService = ss_conn_usb_driver, %SPSVCINST_ASSOCSERVICE%, ssud.AddService

[ssud.Install.CoInst.NT.Wdf]
KmdfService = ss_conn_usb_driver, ss_conn_usb_driver_wdfsect

[ssud.Install.CoInst.NT.CoInstallers]
AddReg=CoInstaller_AddReg
CopyFiles=CoInstaller_CopyFiles

[CoInstaller_CopyFiles]
WdfCoInstaller01007.dll

[CoInstaller_AddReg]
HKR,,CoInstallers32,0x00010000, "WdfCoInstaller01007.dll,WdfCoInstaller"

[ssud.Install.NTarm64]
CopyFiles=ssud.NTarm64.CopyFiles

[ssud.Install.NTarm64.Services]
AddService = ss_conn_usb_driver, %SPSVCINST_ASSOCSERVICE%, ssud.AddService

[ssud.NTarm64.CopyFiles]
ss_conn_usb_driver.sys

[ssud.Install.NTarm64.Wdf]
